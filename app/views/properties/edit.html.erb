<h1>Edit Property</h1>
<div class="row">
  <div class="col-md-12">
    <%= form_for @property do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
      <div class="form-group">
        <%= f.label :address1 %>
        <%= f.text_field :address1%>
      </div>
      <div class="form-group">
        <%= f.label :address2 %>
        <%= f.text_field :address2 %>
      </div>
      <div class="form-group">
        <%= f.label :address3 %>
        <%= f.text_field :address3 %>
      </div>
      <div class="form-group">
        <%= f.label :country %>
        <%= f.text_field :country %>
      </div>
      <div class="form-group">
        <%= f.label :city %>
        <%= f.text_field :city %>
      </div>
      <div class="form-group">
        <%= f.label :state %>
        <%= f.text_field :state %>
      </div>
      <div class="form-group">
        <%= f.label :zip %>
        <%= f.text_field :zip %>
      </div>
      <div class="form-group">
        <%= f.label :taxes %>
        <%= f.text_field :taxes %>
      </div>
      <div class="form-group">
        <%= f.label :rent %>
        <%= f.text_field :rent %>
      </div>
      <div class="form-group">
        <%= f.label :notes %>
        <%= f.text_field :notes %>
      </div>
      <div class="form-group">
        <%= f.submit "Save" %>
      </div>
    </div>
  <% end %>
</div>
